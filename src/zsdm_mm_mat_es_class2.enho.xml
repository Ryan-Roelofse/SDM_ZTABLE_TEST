      <item>exit.</item>
      <item>else.</item>
      <item>insert &lt;custom&gt; into table me-&gt;MT_CUSTOM_TEST_spec.</item>
      <item>endif.</item>
      <item>endloop.</item>
      <item>endif.</item>
      <item>*    endif.</item>
